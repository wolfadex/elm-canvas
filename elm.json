{
  "type": "package",
  "name": "wolfadex/elm-canvas",
  "summary": "A 2D drawing API based on DOM Canvas & ianmackenzie/elm-geometry.",
  "license": "BSD-3-Clause",
  "version": "1.0.0",
  "exposed-modules": [
    "Canvas",
    "Canvas.Settings",
    "Canvas.Settings.Advanced",
    "Canvas.Settings.Line",
    "Canvas.Settings.Text",
    "Canvas.Texture"
  ],
  "elm-version": "0.19.0 <= v < 0.20.0",
  "dependencies": {
    "avh4/elm-color": "1.0.0 <= v < 2.0.0",
    "elm/core": "1.0.2 <= v < 2.0.0",
    "elm/json": "1.1.1 <= v < 2.0.0",
    "ianmackenzie/elm-geometry": "3.9.1 <= v < 4.0.0",
    "ianmackenzie/elm-units": "2.9.0 <= v < 3.0.0"
  },
  "test-dependencies": {
    "elm/html": "1.0.0 <= v < 2.0.0",
    "elm-explorations/test": "1.0.0 <= v < 2.0.0"
  }
}
